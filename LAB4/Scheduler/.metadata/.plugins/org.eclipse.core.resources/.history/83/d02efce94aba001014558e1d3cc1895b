#include "traffic_light_stop.h"

void fsm_traffic_stop(){
	if(system_state != STOP_MODE) return;
	switch(status_stop){
	case INIT_STOP_ROAD:
		break;
	case SELECT_ROAD:
		break;
	case INIT_ROAD1:
		break;
	case ROAD1:
		break;
	case INIT_ROAD2:
		break;
	case ROAD2:
		break;
	default:
		break;
	}
}
